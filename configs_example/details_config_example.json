{
  "general_path": "details",                                    - general path to the folder with details
  "data":
  [
    {
      "name": "Board",                                          - name of the detail
      "mesh": "board/Board.ply",                                - path to mesh .ply or .obj
      "idx": "B",                                               - index name of detail on the board, all MUST be distinct
      "color": [255, 231, 166],                                 - OPTIONAL, rgb if no texture on the object
      "rotate": [-1.57079632679489662, 0, -1.5707963267948966]  - OPTIONAL, base rotation value (x, y, z)
    },
    {
      "name": "Forward",
      "mesh": "forward/forward.obj",
      "idx": "F"
    },
    ...
  ],
  "labels2id": {"F": 1},                                        - labels to id of object for detection (start from 1)
  "id2labels": {"0": "Forward"},                                - id to lables (start from 0)
  "labelsOfBigShapes": ["F"]                                    - idx values of big (not 1x1 shapes)
}